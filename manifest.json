{
    "manifest_version": 3,
    "name": "Coto Script",
    "version": "0.10",
    "description": "Coto Script",
    "author": "Coto",
    "content_scripts":[
        {
            "matches":["*://*.courir.com/*"],
            "js":["script/content_script.js"]
        }
    ],
    "action": {
        "default_title": "Coto Script",
        "default_popup": "popup/cotoscript-popup.html",
        "default_icon": {
            "16": "icons/icon.png",
            "24": "icons/icon.png", 
            "32": "icons/icon.png"
        }
    },
    "icons": {
        "16": "icons/icon.png",
        "24": "icons/icon.png", 
        "32": "icons/icon.png"
    },
    "minimum_chrome_version":"88"
}